<h2>Welcome, {{whoAmI.user.username}}</h2>
<div class="row">
  <div class="col s6">
    <h3>{{user.firstName}}</h3>
    <h4>{{user.email}}</h4>
    <h3>Collection:</h3>
    <ul>
      {{#each user.collection as |game|}}
      <li>{{game-display-tile game=game}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="col s6">
    {{google-map user=user showDistance=showDistance}}
    <button type="button" class='btn' name="button" {{action 'showDistance'}}>Find distance from me</button>
  </div>

</div>
{{yield}}
